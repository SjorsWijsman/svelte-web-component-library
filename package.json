{
    "name": "svelte-web-components",
    "description": "A base template for building a shareable web components library with Vite + Svelte",
    "version": "0.0.0",
    "type": "module",
    "module": "dist/lib/svelte-web-components.js",
    "main": "dist/lib/svelte-web-components.umd.js",
    "scripts": {
        "start": "npm run dev -s",
        "dev": "vite --config vite.demo.config.js",
        "build": "vite build",
        "build:demo": "vite build --config vite.demo.config.js",
        "build:legacy": "npx babel ./dist/svelte-web-components.js --out-file ./dist/svelte-web-components.legacy.js",
        "preview": "vite preview --config vite.demo.config.js",
        "prepublishOnly": "npm run build -s"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.9",
        "@babel/core": "^7.24.0",
        "@babel/preset-env": "^7.24.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.2",
        "@vitejs/plugin-legacy": "^5.3.1",
        "sass": "^1.71.1",
        "svelte": "^4.2.12",
        "svelte-check": "^3.6.6",
        "svelte-preprocess": "^5.1.3",
        "vite": "^5.1.4"
    },
    "files": [
        "dist/lib"
    ]
}
